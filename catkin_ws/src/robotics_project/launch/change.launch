<?xml version="1.0"?>
<launch>
  <!-- start Webots -->
  <arg name="no_gui" default="false," doc="Start Webots with minimal GUI"/>
  <include file="$(find robotics_project)/launch/webots.launch">
    <arg name="mode" value="realtime"/>
    <arg name="no_gui" value="$(arg no_gui)"/>
    <arg name="world" value="$(find robotics_project)/worlds/project_world.wbt"/>
  </include>

  <arg name="auto_close" default="false" doc="Startup mode"/>
  <node name="change" pkg="robotics_project" type="change" required="$(arg auto_close)"/>
</launch>
